{
  "name": "llog",
  "private": true,
  "version": "0.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "api:dev": "wrangler dev api/index.ts --config wrangler-api.jsonc",
    "api:push": "wrangler deploy api/index.ts --config wrangler-api.jsonc --minify",
    "api:types": "wrangler types --config wrangler-api.jsonc --env-interface CloudflareBindings",
    "app:dev": "bun app:watch && expo start",
    "app:doc": "bunx expo-doctor",
    "app:native:build": "expo prebuild --clean",
    "app:theme": "bun scripts/generate-theme.ts",
    "app:watch": "watchman watch-del $(pwd) && watchman watch-project $(pwd)",
    "app:web:build": "expo export --platform web --output-dir web",
    "app:web:push": "wrangler pages deploy web",
    "db:pull": "instant-cli pull",
    "db:push": "instant-cli push",
    "format": "prettier --write .",
    "lint": "expo lint",
    "up": "bun update --latest && bun i tailwindcss@3.4.17 && expo install --fix"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.0",
    "@instantdb/admin": "^0.19.15",
    "@instantdb/react-native": "0.19.15",
    "@legendapp/list": "^1.0.15",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.4.3",
    "@react-navigation/stack": "^7.3.3",
    "@rn-primitives/alert-dialog": "^1.2.0",
    "@rn-primitives/dropdown-menu": "^1.2.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "^1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/toggle": "^1.2.0",
    "@rn-primitives/types": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "expo": "^53.0.10",
    "expo-application": "^6.1.4",
    "expo-image": "^2.2.0",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.0.7",
    "expo-secure-store": "^14.2.3",
    "expo-system-ui": "^5.0.8",
    "hono": "^4.7.11",
    "lucide-react-native": "^0.513.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-sortables": "^1.7.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/metro-runtime": "^5.0.4",
    "@types/chroma-js": "^3.1.1",
    "@types/react": "~19.0.10",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chroma-js": "^3.1.2",
    "eslint": "^9.28.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "expo-dev-client": "^5.2.0",
    "instant-cli": "0.19.15",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "typescript": "^5.8.3",
    "wrangler": "^4.19.1"
  },
  "resolutions": {
    "react": "19.0.0"
  },
  "patchedDependencies": {
    "uuid@9.0.1": "patches/uuid@9.0.1.patch"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
