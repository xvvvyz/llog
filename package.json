{
  "name": "llog",
  "private": true,
  "version": "0.0.1",
  "main": "expo-router/entry",
  "scripts": {
    "db:push:dev": "bun scripts/with-env.ts .dev.vars.development instant-cli push",
    "db:push:prod": "bun scripts/with-env.ts .prod.vars.production instant-cli push",
    "expo:dev": "bun watchman && expo start",
    "expo:doc": "bunx expo-doctor",
    "expo:native:build": "expo prebuild",
    "expo:web:build": "expo export --platform web --output-dir web",
    "format": "prettier --write .",
    "lint": "expo lint",
    "theme": "bun scripts/generate-theme.ts",
    "up": "bun update --latest && bun i tailwindcss@3.4.17 && expo install --fix",
    "watchman": "watchman watch-del $(pwd) && watchman watch-project $(pwd)",
    "worker:dev": "wrangler dev --env development",
    "worker:push:prod": "wrangler deploy --env production --name llog",
    "worker:secrets:push:prod": "wrangler secret bulk .dev.vars.production",
    "worker:types:dev": "wrangler types --env development --env-interface CloudflareEnv"
  },
  "dependencies": {
    "@cloudflare/ai-utils": "^1.0.1",
    "@google/genai": "^1.4.0",
    "@hono/zod-validator": "^0.7.0",
