{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_date": "2025-06-07",
  "name": "llog",
  "main": "api/index.ts",
  "assets": { "directory": "web" },
  "env": {
    "development": {
      "r2_buckets": [{ "binding": "R2", "bucket_name": "llog" }],
      "vars": { "ENV": "development" },
    },
    "production": {
      "r2_buckets": [{ "binding": "R2", "bucket_name": "llog" }],
      "vars": { "ENV": "production" },
    },
  },
}
